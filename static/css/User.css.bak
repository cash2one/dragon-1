.Invite-l span { display:block; font-size:14px; color:#909090; line-height:44px;}
.Invite-l form input[type="text"]{ width:405px; height:31px; border:1px solid #eee; color:#00bc8d; font-size:13px; text-indent:34px; line-height:31px; outline:none;}
.Invite-l form input[type="button"] { width:92px; height:33px; border:1px solid #f08519; background:#f08519; font-size:15px; color:#fff;}
.Invite-l h6 { font-size:14px; color:#000000; line-height:30px;}
