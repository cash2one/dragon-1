.Invite-r p { font-size:14px; color:#666666 ; display:block;}
.Invite-r i { font-weight:bold; color:#f08519;}
.Rivicont { float:left; width:100%; height:auto; margin-top:18px; padding-bottom:14px; padding-left:13px;}
.Rivicont ul { width:106%;}
.Rivicont li { float:left; width:194px; height:207px; position:relative; margin-right:26px;}
.Fare { position:absolute; bottom:0; width:100%; height:62px; background:rgba(0,0,0,.7); padding-top:8px;}
.Fare span { font-size:15px; color:#fff; display:block; text-align:left; line-height:26px; text-indent:10px;}
.Fare p { font-size:15px; color:#fff; text-align:left; display:block; line-height:26px; text-indent:10px;}
.Inlet-go { float:left; width:100%; height:auto; padding-top:15px; border-top:1px solid #ebebeb;}
.Inlet-go p { text-align:center; font-size:14px; color:#afafaf ; line-height:46px;}
.Inlet-go p a { color:#008cd6; font-size:14px;}
.Online { float:left; width:860px; height:auto; padding:7px 11px; background:#fff; text-align:left; border:1px solid #eee;}
.Online strong { font-weight:400; font-size:22px; color:#008fd7; line-height:66px; float:left; border-right:1px solid #ebebeb; width:122px; margin-top:26px;}
.Online-M { float:left; margin-left:28px; width:500px; height:auto; margin-top:3px;} 
