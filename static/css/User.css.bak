.Detail-T li:hover { background:#f08519; color:#fff;}
.Detail-T .on { background:#f08519; color:#fff;}
.Detail-B { position:relative;float:left; width:100%; height:380px;}
a.chakan { color:#008fd7; font-size:14px;}
.Detail-B  tr { border:1px solid #ebebeb;}
.Detail-B  th { font-weight:400; font-size:16px; color:#666666;  background:#f8f8f8; text-align:center; border-top:1px solid #fff;}
.Detail-B tr th {height:44px; line-height:44px;}
.Detail-B  tr td { font-size:14px; color:#afafaf; height:44px; line-height:44px;}

.Listpop { position:fixed; width:100%; height:100%; background:url(../images/back-si.png); z-index:1000; display:none;}
.w700v { width:700px; margin:5% auto 0; height:auto; overflow:hidden;}
