.Welfare-td tr th {height:44px; line-height:44px;}
.Welfare-td tr td { font-size:14px; color:#afafaf; height:44px; line-height:44px;}


.Integral { float:left; width:858px; height:auto; padding:24px 12px; border:1px solid #eeeeee; text-align:left; background:#fff;}
